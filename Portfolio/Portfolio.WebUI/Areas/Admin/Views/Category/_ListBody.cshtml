@model IEnumerable<Category>

@foreach (var item in Model)
{
<tr>
    <td>@item.Name</td>
  
    <td class="operation">
        <a asp-action="edit" asp-route-id="@item.Id" class="btn btn-warning">
            <i class="fa fa-pencil"></i>
        </a>
        <a asp-action="detail" asp-route-id="@item.Id" class="btn btn-secondary">
            <i class="fa fa-eye"></i>
        </a>
        @*<a onclick="removeEntity(event,@item.Id, '@item.Title')" class="btn btn-danger">
                <i class="fa fa-trash"></i>
            </a>*@

        <a asp-action="remove" asp-route-id="@item.Id" class="btn btn-danger">
            <i class="fa fa-trash"></i>
        </a>
    </td>
</tr>
}



