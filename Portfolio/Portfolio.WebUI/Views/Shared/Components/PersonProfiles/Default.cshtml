@model PersonProfilesViewModel
<div class="side-bar">

    <!-- Professional Details -->
    <h5 class="tittle">Professional Details</h5>
    <img src="~/assets/images/avatar.jpg" class="img-responsive" alt="">
    <ul class="personal-info">
        <li>
            <p> <span> Name</span> @Model.Person.Name </p>
        </li>
        <li>
            <p> <span> Age</span> @Model.Person.Age Years </p>
        </li>
        <li>
            <p> <span> Location</span> @Model.Person.Location </p>
        </li>
        <li>
            <p> <span> Experience</span> @Model.Person.Experiences Years </p>
        </li> 

        <li>
            <p> <span> Degree</span> @Model.Person.Degree </p>
        </li>  
        <li>
            <p> <span> Career Level</span> @Model.Person.CareerLevel </p>
        </li>
        <li>
            <p> <span> Phone</span> @Model.Person.Phone </p>
        </li>
        <li>
            <p> <span> FAX</span> @Model.Person.Fax </p> 
        </li>
        <li>
            <p> <span> E-mail</span> <a href="#."> @Model.Person.Email</a> </p>
        </li>
        <li>
            <p> <span> Website</span><a href="#."> @Model.Person.Website</a></p>
        </li>
    </ul>

    <!-- Attachments -->
    <h5 class="tittle">Attachments</h5>
    <div class="attach bor-btm padding-25">
        <ul>
            <li>
                <p><img src="~/assets/images/pdf-icon.jpg" alt=""> Curriculum-Vitae.pdf <a href="#."><i class="fa fa-cloud-download"></i></a> <a href="#."><i class="fa fa-eye"></i></a></p>
            </li>
            <li>
                <p><img src="~/assets/images/word-icon.jpg" alt=""> COE.docx <a href="#."><i class="fa fa-cloud-download"></i></a> <a href="#."><i class="fa fa-eye"></i></a></p>
            </li>
        </ul>
    </div>

    <!-- Social Profiles -->
    <h5 class="tittle">Social Profiles</h5>
    <div class="social-icon bor-btm padding-25">
        <ul>
           @foreach (var item in Model.Profiles)
            {
                <li> <a href="@item.Url"><i class="fa-brands fa-@item.ProfileName"></i></a></li>
               
            }  

        </ul>
    </div>

    <!-- Contact Me -->
    <h5 class="tittle">Contact Me</h5>
    <div class="contact padding-25">
        <!-- Success Msg -->
        <div id="contact_message" class="success-msg"> <i class="fa fa-paper-plane-o"></i>Thank You. Your Message has been Submitted</div>

        <!-- FORM -->
        <form role="form" id="contact_form" class="contact-form" method="post" onSubmit="return false">
            <ul class="row">
                <li class="col-sm-12">
                    <label>
                        <input type="text" class="form-control" name="email" id="email" placeholder="Email">
                    </label>
                </li>
                <li class="col-sm-12">
                    <label>
                        <input type="text" class="form-control" name="company" id="company" placeholder="Subject">
                    </label>
                </li>
                <li class="col-sm-12">
                    <label>
                        <textarea class="form-control" name="message" id="message" rows="5" placeholder="Message"></textarea>
                    </label>
                </li>
                <li class="col-sm-12">
                    <button type="submit" value="submit" id="btn_submit" onClick="proceed();">Send Message</button>
                </li>
            </ul>
        </form>
    </div>
</div>